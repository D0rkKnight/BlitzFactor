{
  "name": "blitzfactor",
  "displayName": "BlitzFactor",
  "description": "Refactoring on Steroids",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:blitzfactor.helloWorld"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "blitzFactor.sidePreview",
        "title": "BlitzFactor Side Preview"
      },
      {
        "command": "blitzFactor.printActions",
        "title": "Print Code Actions"
      },
      {
        "command": "blitzFactor.printAST",
        "title": "Print AST"
      }
    ],
    "customEditors": [
      {
        "viewType": "blitzEditors.blitzEditor",
        "displayName": "BlitzEditor",
        "selector": [
          {
            "filenamePattern": "*.js"
          }
        ]
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "blitzFactor.sidePreview",
          "group": "navigation",
          "when": "resourceLangId == javascript"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --config webpack/webpack.config.js",
    "watch": "webpack --watch --config webpack/webpack.config.js",
    "package": "webpack --mode production --devtool hidden-source-map --config webpack/webpack.config.js",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "cosmos": "cosmos",
    "cosmos:export": "cosmos-export"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/react": "^18.0.28",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "react-cosmos": "^5.7.2",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@dnd-kit/core": "^6.0.8",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "dnd-kit": "^0.0.0",
    "html-webpack-plugin": "^5.5.0",
    "nearley": "^2.20.1",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.1",
    "tree-sitter": "^0.20.1",
    "tree-sitter-cli": "^0.20.7",
    "tree-sitter-javascript": "^0.19.0",
    "vscode-dts": "^0.3.3"
  },
  "enabledApiProposals": [
    "envShellEvent"
  ]
}
